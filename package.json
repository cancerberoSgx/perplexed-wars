{
  "name": "perplexed-wars",
  "version": "0.0.1",
  "description": "real-time tower-defense-like game but where you decide if attack or defend.",
  "private": true,
  "dependencies": {
    "pathfinding": "^0.4.18",
    "plugin-container": "^1.0.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "redux": "^4.0.0"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "lint": "tslint src/**/*.ts",
    "lint-fix": "tslint --fix src/**/*.ts",
    "tsc": "rm -rf types docs && tsc --project tsconfig_for_typedoc.json",
    "doc": "rm -rf types docs && tsc --project tsconfig_for_typedoc.json && typedoc --options typedoc.ts",
    "clean": "rm -rf node_modules types docs build"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^9.6.5",
    "@types/react": "^16.3.11",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^5.0.16",
    "@types/redux": "^3.6.0",
    "react-scripts-ts": "2.15.1",
    "typedoc": "^0.11.1",
    "typescript": "^2.8.1"
  }
}
